{
  "name": "burdadirect/symfony-bundle-helper",
  "description": "HBM Helper Bundle - StringSanitizer, S3Helper, BlitlineHelper, HmacHelper.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "HBM\\HelperBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\HBM\\HelperBundle\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4.0",
    "ext-curl" : "*",
    "ext-json" : "*",
    "ext-tidy" : "*",
    "ext-mbstring": "*",
    "symfony/config": "^4.4|^5.2",
    "symfony/dependency-injection": "^4.4|^5.2",
    "symfony/framework-bundle": "^4.4|^5.2",
    "symfony/twig-bundle": "^4.4|^5.2",
    "aws/aws-sdk-php": "^3.52",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit" : "^8.0|^9.0",
    "symfony/phpunit-bridge": "^4.4|^5.2|^6.0"
  }
}
