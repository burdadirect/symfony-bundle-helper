{
  "name": "burdadirect/symfony-bundle-helper",
  "description": "HBM Helper Bundle - StringSanitizer, S3Helper, BlitlineHelper, HmacHelper.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "repositories" : [
    {
      "type": "vcs",
      "url": "git@github.com:burdadirect/codestyle.git"
    }
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "HBM\\HelperBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\HBM\\HelperBundle\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2.0",
    "ext-curl" : "*",
    "ext-json" : "*",
    "ext-tidy" : "*",
    "ext-mbstring": "*",
    "symfony/config": "^6.3",
    "symfony/dependency-injection": "^6.3",
    "symfony/framework-bundle": "^6.3",
    "symfony/twig-bundle": "^6.3",
    "aws/aws-sdk-php": "^3.52",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "burdadirect/codestyle": "dev-main",
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit" : "^9.5",
    "symfony/phpunit-bridge": "^6.0"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
