{
  "name": "burdanews/helper-bundle",
  "description": "HBM Helper Bundle - StringSanitizer, S3Helper, BlitlineHelper, HmacHelper.",
  "author" : [{
    "name" : "Christian Puchinger",
    "email" : "puchinger@burda.com"
  }],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:burdanews/helper-bundle.git"
  },
  "type": "symfony-bundle",
  "autoload": {
    "psr-0": {
      "HBM\\HelperBundle": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php" : ">=7.0",
    "ext-tidy" : "*",
    "ext-curl" : "*",
    "ext-mbstring": "*",
    "symfony/symfony": "~3.0",
    "aws/aws-sdk-php": "~3.16"
  },
  "require-dev": {
    "phpunit/phpunit" : "~5.3",
    "symfony/phpunit-bridge": "~3.0"
  },
  "target-dir": "HBM/HelperBundle"
}
