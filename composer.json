{
  "name": "burdanews/helper-bundle",
  "description": "HBM Helper Bundle - StringSanitizer, S3Helper, BlitlineHelper, HmacHelper.",
  "author" : [{
    "name" : "Christian Puchinger",
    "email" : "puchinger@burda.com"
  }],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:burdanews/helper-bundle.git"
  },
  "type": "symfony-bundle",
  "autoload": {
    "psr-0": {
      "HBM\\HelperBundle": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php" : ">=7.1",
    "ext-tidy" : "*",
    "ext-curl" : "*",
    "ext-mbstring": "*",
    "symfony/config": "^3.0|^4.0",
    "symfony/dependency-injection": "^3.0|^4.0",
    "symfony/framework-bundle": "^4.0",
    "aws/aws-sdk-php": "^3.52"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit" : "^6.5|^7.0",
    "symfony/phpunit-bridge": "^3.0|^4.0"
  },
  "target-dir": "HBM/HelperBundle"
}
