{
  "name": "burdanews/symfony-bundle-helper",
  "description": "HBM Helper Bundle - StringSanitizer, S3Helper, BlitlineHelper, HmacHelper.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "HBM\\HelperBundle\\": "HBM/HelperBundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\HBM\\HelperBundle\\": "tests/HBM/HelperBundle"
    }
  },
  "require": {
    "php" : ">=7.3",
    "ext-tidy" : "*",
    "ext-json" : "*",
    "ext-curl" : "*",
    "ext-mbstring": "*",
    "symfony/config": "^3.4|^4.4",
    "symfony/dependency-injection": "^3.4|^4.4",
    "symfony/framework-bundle": "^3.4|^4.4",
    "symfony/twig-bundle": "^3.4|^4.4",
    "aws/aws-sdk-php": "^3.52",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit" : "^7.0|^8.0",
    "symfony/phpunit-bridge": "^3.4|^4.4"
  }
}
