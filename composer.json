{
  "name": "burdanews/symfony-bundle-helper",
  "description": "HBM Helper Bundle - StringSanitizer, S3Helper, BlitlineHelper, HmacHelper.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-0": {
      "HBM\\HelperBundle": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php" : ">=7.1",
    "ext-tidy" : "*",
    "ext-curl" : "*",
    "ext-mbstring": "*",
    "symfony/config": "^3.4|^4.2",
    "symfony/dependency-injection": "^3.4|^4.2",
    "symfony/framework-bundle": "^3.4|^4.2",
    "symfony/twig-bundle": "^3.4|^4.2",
    "aws/aws-sdk-php": "^3.52",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit" : "^6.5|^7.0",
    "symfony/phpunit-bridge": "^3.0|^4.0"
  },
  "target-dir": "HBM/HelperBundle"
}
